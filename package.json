{
  "name": "angular2-material-lite",
  "version": "0.0.0",
  "license": "MIT",
  "main": "server/app.js",
  "scripts": {
    "ng": "ng",
    "startnew": "concurrently --kill-others \"npm run build && npm run _server:run\" \"mongod\"",
    "start": "concurrently --kill-others \"ng serve --aot=true --progress=false --progress=false --proxy-config proxy.conf.json\" \"npm run _server:run\" \"mongod\"",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.0",
